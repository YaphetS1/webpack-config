{
  "name": "webpack-config",
  "version": "0.0.1",
  "description": "Example of webpack config",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/karma start",
    "start": "node_modules/.bin/webpack-dev-server --progress",
    "build:aot": "node_modules/.bin/ngc -p tsconfig-aot.json && npm run clear:js",
    "build": "npm run clear:build && node_modules/.bin/webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:jit": "npm run clear:build && node_modules/.bin/webpack --config config/webpack.prod-jit.js --progress --profile --bail",
    "build:jit:win": "npm run clear:build && webpack --config config/webpack.prod-jit.js --progress --profile --bail",
    "server": "node_modules/.bin/lite-server -c config/server.json",
    "clear": "npm run clear:aot && npm run clear:build && npm run clear:js",
    "clear:all": "npm run clear:aot && npm run clear:build && npm run clear:npm",
    "clear:aot": "node_modules/.bin/rimraf aot",
    "clear:js": "node_modules/.bin/rimraf src/**/*.js src/**/*.js.map",
    "clear:npm": "node_modules/.bin/rimraf node_modules",
    "clear:build": "node_modules/.bin/rimraf static",
    "lint": "node_modules/.bin/tslint -c tslint.json src/**/*.ts",
    "deploy:com": "sh deploy/deploy-com.sh",
    "deploy:dev": "sh deploy/deploy-dev.sh"
  },
  "author": "Dmitry Marinin",
  "license": "ISC",
  "dependencies": {
    "@agm/core": "^1.0.0-beta.0",
    "@angular/common": "4.2.2",
    "@angular/compiler": "4.2.2",
    "@angular/compiler-cli": "4.2.2",
    "@angular/core": "4.2.2",
    "@angular/forms": "4.2.2",
    "@angular/http": "4.2.2",
    "@angular/platform-browser": "4.2.2",
    "@angular/platform-browser-dynamic": "4.2.2",
    "@angular/router": "4.2.2",
    "@ngrx/core": "^1.2.0",
    "@ngrx/db": "^2.0.2",
    "@ngrx/effects": "^2.0.3",
    "@ngrx/router-store": "^1.2.6",
    "@ngrx/store": "^2.2.2",
    "core-js": "^2.4.1",
    "ngrx-store-freeze": "^0.1.9",
    "rxjs": "^6.0.0-alpha.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.4.1",
    "@types/jasmine": "^2.5.52",
    "@types/lodash": "^4.14.66",
    "@types/node": "^7.0.31",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.6.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lite-server": "^2.3.0",
    "lodash": "^4.17.4",
    "ngx-mydatepicker": "^2.1.3",
    "node-sass": "^4.5.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.3",
    "typescript": "2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
